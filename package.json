{
  "name": "@arunsrest/hello-component",
  "version": "1.1.0",
  "description": "React components for Home Office layouts and elements",
  "main": "dist/ho-react-components.js",
  "module": "dist/ho-react-components.module.js",
  "style": "src/index.scss",
  "scripts": {
    "pretest:unit-built": "npm run build",
    "test": "npm run test:unit",
    "lint": "eslint . --ext jsx,js",
    "test:unit": "npm run test:unit-built && npm run test:unit-source",
    "test:unit-built": "jest --config ./scripts/jest/config.build.js --env=jsdom",
    "test:unit-source": "jest --config ./scripts/jest/config.source.js --env=jsdom",
    "test:watch": "npm run test:unit-source -- --watch",
    "test:update": "npm run test:unit-source -- --updateSnapshot",
    "build": "npm run build:js",
    "build:js": "rollup -c rollup.config.js",
    "prebuild:css": "mkdir -p ./assets",
    "prepare": "npm run build",
    "audit": "npm audit --audit-level=critical"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arunsrest/hello-component.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arunsrest/hello-component/issues"
  },
  "homepage": "https://github.com/arunsrest/hello-component#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "react-markdown": "^6.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": ">= 16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^22.4.4",
    "jest-cli": "^23.6.0",
    "jest-serializer-html": "^5.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-test-renderer": "^16.5.1",
    "rollup": "0.60.7",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "1.0.1",
    "rollup-plugin-uglify": "3.0.0"
  },
  "files": [
    "components/*",
    "assets/*",
    "src/*",
    "index.js"
  ],
  "publishConfig": {
    "@arunsrest:registry": "https://npm.pkg.github.com"
  }
}
